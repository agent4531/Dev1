<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control">
   <children>
      <Label layoutX="224.0" layoutY="43.0" text="File Selector">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="fileTxt" layoutX="207.0" layoutY="157.0" />
      <Label layoutX="66.0" layoutY="155.0" text="File name: ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="fileCheckBtn" layoutX="256.0" layoutY="243.0" mnemonicParsing="false" onAction="#fileCheck" text="Check" />
   </children>
</AnchorPane>
