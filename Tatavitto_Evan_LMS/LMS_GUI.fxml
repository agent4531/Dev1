<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control">
   <children>
      <Button fx:id="newBtn" layoutX="50.0" layoutY="25.0" mnemonicParsing="false" onAction="#newOnClick" prefHeight="100.0" prefWidth="200.0" text="New Book" />
      <Button fx:id="removeBtn" layoutX="450.0" layoutY="25.0" mnemonicParsing="false" onAction="#rmOnClick" prefHeight="100.0" prefWidth="200.0" text="Remove Book" />
      <Button fx:id="allBtn" layoutX="250.0" layoutY="150.0" mnemonicParsing="false" onAction="#allOnClick" prefHeight="100.0" prefWidth="200.0" text="All Books" />
      <Button fx:id="outBtn" layoutX="50.0" layoutY="275.0" mnemonicParsing="false" onAction="#outOnClick" prefHeight="100.0" prefWidth="200.0" text="Check Out" />
      <Button fx:id="inBtn" layoutX="450.0" layoutY="275.0" mnemonicParsing="false" onAction="#inOnClick" prefHeight="100.0" prefWidth="200.0" text="Check In" />
   </children>
</AnchorPane>
